Algoritmo "paraexemplo"

Var
soma , qtdNum, numDig : inteiro


Inicio

      escreva("Quantos números serão digitados ?")
      leia(qtdNum)

      para qtdNum de 1 ate qtdNum faca
           escreval("Digite um numero : ")
           leia(numDig)
           
           soma <- soma + numDig
      fimpara
      
      escreva("SOMA = ", soma)



Fimalgoritmo